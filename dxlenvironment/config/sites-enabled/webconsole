server {
    listen 9443;
    client_max_body_size 100m;
    ssl on;
    ssl_certificate /opendxl/keystore/cloudcmd-bundle.crt;
    ssl_certificate_key /opendxl/keystore/cloudcmd.key;
    access_log /opendxl/logs/cloudcmd_access.log;
    location / {
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
        proxy_pass http://127.0.0.1:8000/;
    }
}
